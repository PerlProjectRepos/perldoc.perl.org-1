      <main class="row main-content pb-5 pt-5">
        <div class="col-sm-8 offset-sm-2">
          <div class="row">
            <div class="col-sm-12">
                <div id="breadcrumbs">
                  [% INCLUDE breadcrumb.tt %]
                </div>
            </div>
          </div>
          <div class="row">
              <aside class="col-sm-4 sidebar">
                <h1>[% pagename %]</h1>
                <ul>
                  [% FOREACH page IN section_pages %]
                    <li><a href="[% page.link %]">[% page.name %]</a>
                    [% IF page.title %]
                      - [% page.title %]
                    [% END %]
                  [% END %]
                </ul>
              </aside>
              <article class="col-sm-8 content">
                <div class="documentation-wrapper">
                  <div id="page_name">
                    <h1>[% pagename %]</h1>
                  </div>
                  <div id="perl_version">
                    Perl 5 version [% perl5_version %] documentation
                  </div>
                  [% INCLUDE $content_tt %]
                </div>
              </article>
        </div>        
      </div>
